# Solution 1

def integerDivision(a,b): 
    print(a//b)


def floatDivision(a,b): 
    print(a/b)

if __name__ == '__main__':
    a = int(input())
    b = int(input())
    integerDivision(a,b)
    floatDivision(a,b)
    
    
    
#Solution 2: 

def dividing(a,b): 
    print(int(a)/int(b))
    print(float(a)/float(b))
    
a = int(input())
b = int(input())
dividing(a,b)
    
    
# Solution 3: 

a = int(input())
b = int(input())
print("%d\n%d" %(a//b, a/b))
